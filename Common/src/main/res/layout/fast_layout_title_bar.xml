    xmlns:TitleBarView="http://schemas.android.com/apk/res-auto"
    android:id="@+id/title_bar"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_title_height"
    android:background="@color/colorTitleBackground"
    TitleBarView:title_actionTextColor="@color/colorTitleText"
    TitleBarView:title_dividerBackground="@color/colorTitleDivider"
